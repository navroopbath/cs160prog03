        <activity
            android:name=".timer"
            android:label="@string/title_activity_prezi_hub"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".statsPrezi"
            android:label="@string/title_activity_stats_prezi"
            android:theme="@style/AppTheme.NoActionBar" >
